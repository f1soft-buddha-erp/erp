     * @return The MachineryResponseDTO parse from Machinery
     */
    public static MachineryResponseDTO parseToMachineryResponse(Machinery m) {
        MachineryResponseDTO response = new MachineryResponseDTO(m.getId());
        response.setAcquiredDate(m.getAcquiredDate());
        response.setAcquisitionCost(m.getAcquisitionCost());
        response.setActive(m.getActive());
        response.setAssignedTo(m.getAssignedTo());
        response.setCurrentValue(m.getCurrentValue());
        response.setDepreciationRate(m.getDepreciationRate());
        response.setLastMaintenanceDate(m.getLastMaintenanceDate());
        response.setMachineCompanyName(m.getMachineCompanyName());
        response.setMachineName(m.getMachineName());
        response.setMachineType(m.getMachineType());
        response.setMaintenanceCyclePerYear(m.getMaintenanceCyclePerYear());
        response.setMaintenanceDate(m.getMaintenanceDate());
        response.setRegistrationNo(m.getRegistrationNo());

        return response;
    }

